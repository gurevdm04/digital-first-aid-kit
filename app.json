{
  "expo": {
    "name": "MedTracker",
    "slug": "digital-first-aid-kit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "digitalfirstaidkit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d3cb15f0-9130-4dbe-a692-b9df79baf9e8"
      }
    },
    "android": {
      "package": "com.gurevdm04.digitalfirstaidkit"
    },
    "ios": {
      "bundleIdentifier": "com.gurevdm04.digitalfirstaidkit"
    }
  }
}
